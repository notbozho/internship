{
    "name": "Song Downloader",
    "description": "Downloads songs from YouTube/Spotify",
    "manifest_version": 3,
    "version": "0.0.1",
    "host_permissions": [
        "https://*/*",
        "http://*/*"
    ],
    "permissions": [
        "tabs",
        "storage",
        "downloads"
    ],
    "action": {
        "default_popup": "popup.html"
    }
}